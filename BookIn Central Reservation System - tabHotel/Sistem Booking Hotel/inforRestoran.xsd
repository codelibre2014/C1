                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>/*
declare 
@NoMeja int  = 3
,@NoPemesanan int = 25
*/
SELECT
                         hres.NoMeja, res.kamar_no, Item.item_id, Item.item, Item.item_tipe_id, Item.harga, dres.SubBiaya, dres.SubBiaya / Item.harga AS Jumlah, 
                         hres.NoPemesanan, hres.TglPesan, hres.Biaya, hres.flag, ISNULL(Tamu.tamu, '-') AS tamu
FROM            HRestaurant AS hres LEFT OUTER JOIN
                         Reservasi AS res ON hres.reservasi_id = res.reservasi_id INNER JOIN
                         DRestaurant AS dres ON hres.NoPemesanan = dres.NoPemesanan INNER JOIN
                         Item ON dres.NoItem = Item.item_id LEFT OUTER JOIN
                         Tamu ON res.tamu_id = Tamu.tamu_id
WHERE        (hres.NoPemesanan = @NoPemesanan)</CommandText>
