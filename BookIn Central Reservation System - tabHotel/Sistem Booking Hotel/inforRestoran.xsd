                      <Parameter AllowDbNull="false" AutogeneratedName="NoPemesanan" ColumnName="NoPemesanan" DataSourceName="tabHotel.dbo.HRestaurant" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@NoPemesanan" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="NoPemesanan" SourceColumnNullMapping="false" SourceVersion="Current" />
